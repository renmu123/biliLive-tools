    "better-sqlite3": "catalog:",
    "bufferutil": "^4.0.8",
    "electron-context-menu": "4.1.1",
    "electron-log": "^5.2.3",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.9",
    "font-ls": "catalog:",
    "@renmu/node-shazam": "^2.0.3",
    "ntsuspend": "catalog:",
    "semver": "^7.6.3",
    "trash": "^9.0.0",
    "utf-8-validate": "^6.0.5",
    "wavesurfer.js": "^7.11.1",
    "music-segment-detector": "^0.3.1"
  },
  "devDependencies": {
    "@biliLive-tools/http": "workspace:*",
    "@biliLive-tools/shared": "workspace:*",
    "@biliLive-tools/types": "workspace:*",
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/rebuild": "^4.0.1",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/better-sqlite3": "^7.6.13",
    "@vicons/fluent": "^0.13.0",
    "@vicons/ionicons5": "^0.13.0",
    "@vicons/material": "^0.13.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vueuse/components": "^13.0.0",
    "@vueuse/core": "^13.0.0",
    "artplayer": "^5.2.1",
    "artplayer-plugin-assjs": "^0.1.3",
    "artplayer-plugin-danmuku": "5.1.4",
    "artplayer-plugin-hls-control": "^1.0.1",
    "crypto-js": "^4.2.0",
    "driver.js": "^1.3.1",
    "electron": "39.2.3",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.32.0",
    "file-saver": "^2.0.5",
    "filenamify": "^7.0.0",
    "flv.js": "^1.6.2",
    "hls.js": "^1.5.17",
    "hotkeys-js": "^3.13.7",
