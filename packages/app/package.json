{
  "name": "biliLive-tools",
  "version": "3.9.0",
  "description": "B站直播录制处理工具",
  "main": "./out/main/index.js",
  "author": "renmu123",
  "license": "GPL-3.0",
  "homepage": "https://github.com/renmu123/biliLive-tools",
  "type": "module",
  "scripts": {
    "dev": "electron-vite dev",
    "devMain": "electron-vite dev --watch",
    "start": "electron-vite preview",
    "build": "pnpm run typecheck && electron-vite build",
    "build:webui": "pnpm run typecheck && electron-vite build --mode webui",
    "postinstall": "electron-builder install-app-deps",
    "build:app": "pnpm run build && electron-builder --config",
    "build:app:no-ffmpeg": "pnpm run build && electron-builder --config electron-builder-no-ffmpeg.yml",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
