  "license": "LGPL",
  "dependencies": {
    "@renmu/fluent-ffmpeg": "2.3.3",
    "fast-xml-parser": "^4.5.0",
    "filenamify": "^6.0.0",
    "mitt": "catalog:",
    "string-argv": "^0.3.2",
    "lodash-es": "catalog:",
    "axios": "catalog:",
    "fs-extra": "^11.2.0",
    "ejs": "^3.1.10"
  }
