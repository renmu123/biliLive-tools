{
  "name": "@bililive-tools/bilibili-recorder",
  "version": "1.8.0",
  "description": "bililive-tools bilibili recorder implemention",
  "main": "./lib/index.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./lib/index.js"
    },
    "./*.js": {
      "types": "./src/*.ts",
      "development": "./src/*.ts",
      "default": "./lib/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "bilibili",
    "recorder",
    "bililive-tools",
    "直播",
    "录制",
    "哔哩哔哩"
  ],
  "repository": "https://github.com/renmu123/biliLive-tools/tree/master/packages/BilibiliRecorder",
  "author": "renmu123",
  "license": "LGPL",
  "dependencies": {
    "@bililive-tools/manager": "workspace:^",
    "blive-message-listener": "^0.5.1",
    "mitt": "catalog:",
    "tiny-bilibili-ws": "^1.0.2",
    "lodash-es": "catalog:",
    "axios": "catalog:"
  }
}
