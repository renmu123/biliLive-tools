{
  "name": "@bililive-tools/manager",
  "version": "1.5.0",
  "description": "Batch scheduling recorders",
  "main": "./lib/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./lib/index.js"
